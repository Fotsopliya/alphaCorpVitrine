datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model admins {
    id           Int      @id @default(autoincrement())
    email        String   @unique
    username     String   @unique
    password     String
    created_at   DateTime @default(now())
    updated_at   DateTime @updatedAt
}